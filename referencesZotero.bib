@article{beal2015,
	title = {{ESM} 2.0: {State} of the {Art} and {Future} {Potential} of {Experience} {Sampling} {Methods} in {Organizational} {Research}},
	volume = {2},
	issn = {2327-0608, 2327-0616},
	shorttitle = {{ESM} 2.0},
	url = {https://www.annualreviews.org/doi/10.1146/annurev-orgpsych-032414-111335},
	doi = {10.1146/annurev-orgpsych-032414-111335},
	abstract = {The use of experience sampling methods (ESM) and related techniques has exploded in organizational research. The goals of this review are to provide a focused perspective on the state of the art in using ESM and set the stage for what ESM will look like in the years to come. First, I provide a conceptually based discussion of exactly what is and what is not ESM. Next, I discuss the more advantageous elements of ESM that have surfaced from the range of disciplines that enjoy its use (e.g., reduced memory and methods biases), followed by the inevitable challenges that have sometimes limited its utility (e.g., issues with repeated assessment, missing data, and internal validity). Finally, I discuss three innovations of ESM (e.g., trait assessment, expansion to higher levels of analysis, and connection to big data) that seem likely to ensure its continued and expanded influence as a common tool for examining not only within-person psychological processes at work, but higher levels of analysis as well.},
	language = {en},
	number = {1},
	urldate = {2023-01-08},
	journal = {Annual Review of Organizational Psychology and Organizational Behavior},
	author = {Beal, Daniel J.},
	month = apr,
	year = {2015},
	pages = {383--407},
}

