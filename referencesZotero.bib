
@article{vandermaas2003,
	title = {Sudden {Transitions} in {Attitudes}},
	volume = {32},
	issn = {0049-1241, 1552-8294},
	url = {http://journals.sagepub.com/doi/10.1177/0049124103253773},
	doi = {10.1177/0049124103253773},
	abstract = {Both the dynamic approach and catastrophe modeling have been warmly welcomed in research on attitudes and opinions. In this article, the authors discuss a general methodology for testing catastrophe models and apply it to the dynamics of attitude formation and change. First, by making use of the so-called catastrophe flags, converging support for the catastrophe model can be attained. Each flag relates to a specific hypothesis about attitudinal change. Second, fitting stochastic catastrophe models to data enables one to carry out a direct test of catastrophe models. Results of analyzing large data sets on political attitudes support the validity of the general catastrophe model of attitude change in which transitions in attitudes are a function of involvement and information. Present results suggest that in the case of political attitudes, involvement might well be correlated with attitude. A more refined approach to the measurement of information and involvement is suggested.},
	language = {en},
	number = {2},
	urldate = {2023-01-08},
	journal = {Sociological Methods \& Research},
	author = {van der Maas, Han L. J. and Kolstein, Rogier and van der Pligt, Joop},
	month = nov,
	year = {2003},
	pages = {125--152},
}

